def main()
  write_empty(3)

  write_int(0)
  write_int(010)
  write_int(2147483647)
  write_empty(2)

  write_int(0x0)
  write_int(0x010)
  write_int(0x7FFFFFFF)
  write_int(0x7fffffff)
  write_empty(2)

  write_int(0o0)
  write_int(0o010)
  write_int(0o17777777777)
  write_empty(2)

  write_int(0b0)
  write_int(0b010)
  write_int(0b1111111111111111111111111111111)
  write_empty(2)

  write_ints([3])
  write_ints([3, 1, 4])
end

def write_empty(n: Int)
  for i in n
    write_ints([])
  end
end
